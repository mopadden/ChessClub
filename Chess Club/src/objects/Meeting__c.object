        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Meetings</pluralLabel>
    <recordTypeTrackHistory>true</recordTypeTrackHistory>
    <recordTypes>
        <fullName>Meeting</fullName>
        <active>true</active>
        <description>Meeting</description>
        <label>Meeting</label>
        <picklistValues>
            <picklist>Meeting_Type</picklist>
            <values>
                <fullName>Committee</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Competition</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Practice</fullName>
                <default>true</default>
            </values>
        </picklistValues>
    </recordTypes>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>CommiteeMeetingSupervision</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c,&quot;Committee&quot;), Supervisors__c != 0)</errorConditionFormula>
        <errorMessage>Supervisors not needed for committee meeting</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NonCompetitionGames</fullName>
        <active>true</active>
        <description>Check to make sure that games are held at competitions</description>
        <errorConditionFormula>AND(NOT(Competition__c),OR( Losses__c &gt; 0, Wins__c &gt; 0 ) )</errorConditionFormula>
        <errorMessage>Games should be held at competition meetings</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Players_Planning</fullName>
        <active>true</active>
        <description>Players at committee meeting</description>
        <errorConditionFormula>AND( Planning__c ,  Players__c &gt; 0 )</errorConditionFormula>
        <errorMessage>Players should not attend planning meetings.</errorMessage>
    </validationRules>
</CustomObject>
